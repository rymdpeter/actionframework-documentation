<!DOCTYPE html>
<html>
<head>
  <title>
    Guidelines
  </title>
  <style>
    {{{styles}}}
  </style>
</head>
<body>
  {{#each sections}}
  <article>
    <h1>{{@key}}</h1>
    {{#each .}}
    <section id="{{name}}">
      <h2>{{title}}</h2>
        {{{content}}}
    </section>
    {{/each}}
  </article>
  {{/each}}
  <script>
    {{{scripts}}}
  </script>
</body>
</html>
